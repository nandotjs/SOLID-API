    "npm-run-all": "4.1.5",
    "prisma": "5.7.1",
    "supertest": "6.3.4",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.1.3"
  },
  "dependencies": {
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.7.1",
